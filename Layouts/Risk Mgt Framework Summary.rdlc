                <Paragraphs>
                  <Paragraph>
                    <TextRuns>
                      <TextRun>
                        <Value EvaluationMode="Auto">2.3. RISK IDENTIFICATION METHODS</Value>
                        <Style>
                          <FontStyle>Normal</FontStyle>
                          <FontWeight>Bold</FontWeight>
                          <TextDecoration>Underline</TextDecoration>
                          <Color>#000000</Color>
                        </Style>
